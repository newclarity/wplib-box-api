{
  "namespaces": {
    "v1": {
      "routes": {
        "cache": {
          "methods": {
            "delete": {
              "command": "cache-flush",
              "message": "Flush Caches"
            }
          }
        },
        "projects": {
          "methods": {
            "get": {
              "command": "sites-list",
              "message": "Projects List"
            }
          }
        },
        "project\/{hostname}": {
          "message": "Adding project",
          "methods": {
            "get": {
              "command": "site-info",
              "message": "Site Resource"
            },
            "post": {
              "command": "site-add",
              "message": "Add site"
            },
            "delete": {
              "command": "site-delete",
              "message": "Delete Site"
            }
          }
        },
        "project\/{hostname}\/object-cache": {
          "methods": {
            "post": {
              "command": "enable-object-caching",
              "message": "Enable object caching"
            },
            "delete": {
              "command": "disable-object-caching",
              "message": "Disable object caching"
            }
          }
        },
        "project\/{hostname}\/backup": {
          "command": "site-backup",
          "methods": {
            "post": {
              "command": "site-backup",
              "message": "Backup project"
            }
          }
        }
      }
    }
  }
}